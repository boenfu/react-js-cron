{
  "name": "react-js-cron-semi",
  "version": "1.4.3",
  "description": "A React cron editor with semi inspired by jqCron",
  "author": "boenfu <1997@boenfu.cn> (https://github.com/boenfu/)",
  "license": "MIT",
  "keywords": [
    "react",
    "reactjs",
    "js",
    "cron",
    "crontab",
    "editor",
    "widget",
    "generator",
    "semi",
    "semi-design",
    "cronjob",
    "schedule",
    "parser"
  ],
  "homepage": "https://github.com/boenfu/react-js-cron#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boenfu/react-js-cron.git"
  },
  "bugs": {
    "url": "https://github.com/boenfu/react-js-cron/issues"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "build": "del dist && yarn build:babel && yarn build:types",
    "build:babel": "concurrently \"yarn build:babel:esm\" \"yarn build:babel:cjs\"",
    "build:babel:cjs": "BABEL_ENV=cjs babel --extensions \".ts,.tsx\" --copy-files src --out-dir dist/cjs --presets=@babel/env",
    "build:babel:esm": "BABEL_ENV=esm babel --extensions \".ts,.tsx\" --copy-files src --out-dir dist/esm",
    "build:types": "tsc",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "prettier": "prettier --write 'src/**/*.{ts?(x),css}'",
    "prepublishOnly": "yarn build"
  },
  "peerDependencies": {
    "@douyinfe/semi-icons": ">=2.4.0",
    "@douyinfe/semi-ui": ">=2.4.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/plugin-proposal-export-default-from": "7.16.0",
    "@babel/plugin-transform-typescript": "7.16.1",
    "@douyinfe/semi-icons": "^2.4.1",
    "@douyinfe/semi-ui": "^2.4.1",
    "@storybook/addon-docs": "6.4.9",
    "@storybook/addons": "6.4.9",
    "@storybook/preset-create-react-app": "3.2.0",
    "@storybook/react": "6.4.9",
    "@types/node": "16.11.12",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "babel-plugin-transform-es2015-modules-umd": "6.24.1",
    "babel-plugin-typescript-to-proptypes": "2.0.0",
    "concurrently": "6.4.0",
    "del-cli": "4.0.1",
    "eslint": "8.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "4.5.3"
  }
}
